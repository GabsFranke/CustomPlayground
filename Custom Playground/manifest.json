{
    "name": "Custom Playground",
    "version": "6.8.7",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["https://playground.digitalhouse.com/*"],
        "js": ["content.js"]
      }
    ],

    "browser_action": {
      "default_title": "Custom PG",
      "default_popup": "popup.html"
    },

    "permissions": ["tabs", "storage"]
  }

